{
  "name": "ado-codespaces-auth",
  "displayName": "Azure Devops Codespaces Authentication",
  "description": "",
  "publisher": "RJEx",
  "version": "0.0.18",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ado-codespaces-auth.authenticate",
        "title": "Authenticate Azure DevOps"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ADO Codespaces Auth",
      "properties": {
        "ado-auth-helper.postAuthScript": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Script to Execute when auth token is fetched and/or updated"
        }
      }
    }
  },
  "extensionKind": [
    "workspace"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run esbuild-base",
    "esbuild-base": "esbuild ./src/extension.ts ./src/ado-auth-helper.ts --bundle --outdir=out --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "axios": "^1.3.1",
    "libsodium-wrappers": "^0.7.10"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "typescript": "^4.9.4"
  }
}
